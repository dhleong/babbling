{
    "name": "babbling",
    "version": "3.2.0",
    "description": "Cast Streaming service videos to Chromecast",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "engines": {
        "node": ">=12"
    },
    "files": [
        "/dist"
    ],
    "bin": {
        "babbling": "./dist/cli/index.js"
    },
    "scripts": {
        "build": "tsc -p .",
        "check": "npm run lint && npm run build && npm run test",
        "clean": "rm -rf dist",
        "lint": "eslint . --cache --ext .ts",
        "prepublishOnly": "npm run check",
        "test": "mocha -r ts-node/register 'test/**/*-test.ts'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dhleong/babbling.git"
    },
    "keywords": [
        "chromecast",
        "hulu",
        "hbo",
        "prime",
        "youtube"
    ],
    "author": "Daniel Leong",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/dhleong/babbling/issues"
    },
    "homepage": "https://github.com/dhleong/babbling#readme",
    "dependencies": {
        "chakram-ts": "^1.0.0",
        "debug": "^4.3.4",
        "fs-extra": "^10.0.1",
        "jsonwebtoken": "^8.5.1",
        "leven": "^3.1.0",
        "request": "^2.88.2",
        "request-promise-native": "^1.0.9",
        "stratocaster": "^1.0.5",
        "tough-cookie": "^4.0.0",
        "uuid": "^8.3.2",
        "yargs": "^17.3.1",
        "youtubish": "^1.5.1"
    },
    "devDependencies": {
        "@types/chai": "^4.3.0",
        "@types/debug": "^4.1.7",
        "@types/fs-extra": "^9.0.13",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/mdns": "0.0.34",
        "@types/mocha": "^9.1.0",
        "@types/node": "^17.0.21",
        "@types/request-promise-native": "^1.0.18",
        "@types/uuid": "^8.3.4",
        "@types/yargs": "^17.0.10",
        "@typescript-eslint/eslint-plugin": "^5.15.0",
        "@typescript-eslint/parser": "^5.15.0",
        "chai": "^4.3.6",
        "eslint": "^8.11.0",
        "eslint-config-airbnb-typescript": "^16.1.3",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint_d": "^11.1.1",
        "mocha": "^9.2.2",
        "ts-mockito": "^2.6.1",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.2"
    },
    "optionalDependencies": {
        "chromagnon": "^1.2.0"
    }
}
